#include<stdio.h>
#include<math.h>
int main()
{
	int x1,x2,y1,y2;
	float distance;
	printf("Enter the coordinates of 1st num: \n");
	scanf("%d%d",&x1,&y1);
	printf("Enter the coordinates of 2nd num: \n");
	scanf("%d%d",&x2,&y2);
	distance=sqrt(((x2-x1)*(x2-x1)+ (y2-y1)*(y2-y1)));
	printf("The distance between two points is %f \n",distance);
	return 0;
}
