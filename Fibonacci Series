#include <stdio.h>
int main()
{
    int n,i, ft = 0, st = 1, nt;
    printf("Enter the number of terms: ");
    scanf("%d", &n);
    printf("Fibonacci Series: ");
    for (i = 1; i <= n; i++) 
	{
        if (i == 1) 
		{
            printf("%d \t", ft);
            continue;
        }
        if (i == 2) 
		{
            printf("%d \t", st);
            continue;
        }
        nt = ft+ st; 
        ft= st;  
        st = nt;   
        printf("%d \t", nt);

    }

    printf("\n");
    return 0;
}
